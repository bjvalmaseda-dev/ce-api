{
  "extends": ["standard", "prettier"],
  "plugins": ["import", "prettier", "standard", "cypress"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true,
    "cypress/globals": true
  },
  "rules": {
    "space-before-function-paren": 0,
    "new-cap": 0,
    "prettier/prettier": 2
  }
}
